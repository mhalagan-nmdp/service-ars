    <script>
    $(document).ready(function() {
        $("#Demo-BS .btn").click(function() {
            $(this).button('loading').delay(2000).queue(function() {
                $(this).button('reset');
                $(this).dequeue();
            });
        });
    });
    </script>
    <script>
    function highlightArsGroups() {
        $('.highlightGroup').each(function(i) {

            if (this.style.backgroundColor !== "yellow") {
                this.style.backgroundColor = "yellow";
            } else {
                this.style.backgroundColor = "";
            }
        });
    }
    </script>
    <script>
    function getAction(){
        var x = document.getElementById("myFile");
        var txt = "";
        if (x.files.length == 0) {
          var redux = document.getElementById("reduxForm");
          redux.action = "<% ars_redux_url %>";
          redux.method = "GET";
        }else{
          var redux = document.getElementById("reduxForm");
          redux.action = "<% ars_reduxfile_url %>";
          redux.method = "POST";
        }   
    }

    function downloadClient() {
        document.getElementById('submit_modal').submit();
    }

    function more(obj) {
        var content = document.getElementById("showMore");

        if (content.style.display == "none") {
            content.style.display = "";
            obj.innerHTML = "less options";
        } else {
            content.style.display = "none";
            obj.innerHTML = "more options";
        }
    }
    </script>
